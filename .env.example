# Telegram配置
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here
TELEGRAM_BOT_TOKEN=your_bot_token_here

# AI配置（支持DeepSeek、OpenAI等兼容API）
LLM_API_KEY=your_llm_api_key_here
LLM_BASE_URL=https://api.deepseek.com
LLM_MODEL=deepseek-chat

# 频道配置（支持用户名或链接格式，如@channel_name或https://t.me/channel_name，支持多个频道用逗号分隔）
TARGET_CHANNEL=https://t.me/your_channel_here,https://t.me/another_channel_here

# 管理员配置（支持多个ID，用逗号分隔，ID为纯数字）
REPORT_ADMIN_IDS=your_admin_id_here,another_admin_id_here

# 日志级别（DEBUG, INFO, WARNING, ERROR, CRITICAL）
LOG_LEVEL=INFO

# Web管理界面配置
WEB_PORT=8000
WEB_ADMIN_PASSWORD=your_admin_password_here
